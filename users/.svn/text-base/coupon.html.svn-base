		<script src="../js/mui.min.js"></script>
		<script type="text/javascript">
			mui.init();
		    var request_url =  localStorage.getItem('kfj_request_url');//请求地址
		    var request_img_url = localStorage.getItem('kfj_img_url');//图片路径
            var user_id = localStorage.getItem('ifa_login_id');
			mui.plusReady(function () {
			var self = plus.webview.currentWebview();
			//加载页面数据并填充到页面
			mui.post(request_url+'api/',{
				action: 'app_users_youhuiquan_list',
				user_id: user_id 
			},function(data){
				if(data.msg=="1"){
				 document.getElementById("app-weishiyong").innerText ="未使用("+data.weishiyong+")" ;//未使用
				 var html="";
				 var arr_list=data.listweishiyong;			 
				 for(var i = 0; i < arr_list.length; i++){
				 	html +="<div class='single'>";
				 	html +="<div class='left'>";
				 	html +="<p class='p1'>¥&nbsp;<span class='money'>"+arr_list[i].jian_price+"</span></p>";
				 	html +="<p class='p2'>满<span>"+arr_list[i].man_price+"</span>元可用券</p>";
				 	html +="</div>";
				 	html +="<div class='right'>";
				 	html +="<p class='p1'>本品仅限<span>日用品</span>使用</p>";
				 	html +="<p class='p2'>未使用</p>";
				 	html +="<p class='p3'>"+timeString(arr_list[i].start_time)+"-"+timeString(arr_list[i].end_time)+"</p>";
				 	html +="<a class='a1' href='#'>立即使用</a>";
				 	html +="<div class='tags1'></div>";
				 	html +="</div>";
				 	html +="</div>";    
				 }
				 document.getElementById("item1_").innerHTML = html;				
			
				 document.getElementById("app-yishiyong").innerText = "已使用("+data.yishiyong+")";//已使用
				 var html="";
				 var arr_list=data.listyishiyong;			 
				 for(var i = 0; i < arr_list.length; i++){
				 	html +="<div class='single'>";
				 	html +="<div class='left left_bg'>";
				 	html +="<p class='p1'>¥&nbsp;<span class='money'>"+arr_list[i].jian_price+"</span></p>";
				 	html +="<p class='p2'>满<span>"+arr_list[i].man_price+"</span>元可用券</p>";
				 	html +="</div>";
				 	html +="<div class='right'>";
				 	html +="<p class='p1'>本品仅限<span>日用品</span>使用</p>";
				 	html +="<p class='p2 p2_c'>已使用</p>";
				 	html +="<p class='p3'>"+timeString(arr_list[i].start_time)+"-"+timeString(arr_list[i].end_time)+"</p>";
				 	html +="</div>";
				 	html +="</div>"; 
				 }
				 document.getElementById("item2_").innerHTML = html;
				 
				 document.getElementById("app-yiguoqi").innerText = "已过期("+data.yiguoqi+")";//已过期
				 var html="";
				 var arr_list=data.listyiguoqi;			 
				 for(var i = 0; i < arr_list.length; i++){
				 	html +="<div class='single'>";
				 	html +="<div class='left left_bg'>";
				 	html +="<p class='p1'>¥&nbsp;<span class='money'>"+arr_list[i].jian_price+"</span></p>";
				 	html +="<p class='p2'>满<span>"+arr_list[i].man_price+"</span>元可用券</p>";
				 	html +="</div>";
				 	html +="<div class='right'>";
				 	html +="<p class='p1'>本品仅限<span>日用品</span>使用</p>";
				 	html +="<p class='p2'>已过期</p>";
				 	html +="<p class='p3'>"+timeString(arr_list[i].start_time)+"-"+timeString(arr_list[i].end_time)+"</p>";
				 	html +="<div class='tags2'></div>";
				 	html +="</div>";
				 	html +="</div>";   
				 }
				 document.getElementById("item3_").innerHTML = html;
			 
				}else{
					console.log(data.msgbox);
				}
			},'json'
			);
			})
			function timeString(data){
				 var arr=data.split(" ");
				 console.log(arr[0]);
				 return arr[0];
			}
			//跳转到我的消息详情
			function  messageDetails(){
				localStorage.setItem('currentPage', 'messageDetails'); 
				mui.openWindow({
				    url: './message.html', 
				});
			}
